            <string>ds.Todd_Respect ++;</string>
          </onLoadScripts>
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>I'll be there. See you later.</text>
          <def_link>-1</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />

    <dlgPart>
      <lines>
        <dlgLine>
          <owner_ID>0</owner_ID>
          <text>Thank you. I will make sure her service is the best I have ever given.</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

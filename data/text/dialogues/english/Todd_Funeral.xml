          </scriptsOnClick>
        </dlgAnsw>
        <dlgAnsw>
          <text>Good. She deserves it. I'll leave you to your work.</text>
          <def_link>-1</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />

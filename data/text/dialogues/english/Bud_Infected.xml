          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>Don't worry. When I get the chance, I will deal with them in my way.</text>
          <def_link>2877</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />

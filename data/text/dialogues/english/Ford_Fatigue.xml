          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>Yeah. I think it's important.</text>
          <def_link>4537</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>

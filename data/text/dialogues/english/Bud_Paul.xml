          <owner_ID>1</owner_ID>
          <text>&lt;To you&gt;Oh, hey, commander. We were just talking strategy. Interested?</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
        <dlgLine>
          <owner_ID>1</owner_ID>
          <text>We think it might be better to make some noise - maybe a firecracker or something - outside areas before we enter them. </text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
        <dlgLine>
          <owner_ID>1</owner_ID>
          <text>Oh, allez... tu fais vraiment ça ? Le numéro du boy-scout ?</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

        <dlgLine>
          <owner_ID>1</owner_ID>
          <text>Tu vois quelque chose qui te plaît, cow-boy ?</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

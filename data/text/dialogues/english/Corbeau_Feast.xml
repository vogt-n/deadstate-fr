      <answers>
        <dlgAnsw>
          <text>Sounds like a great idea, especially if it helps improve everyone's mood. &lt;-30 food.&gt;</text>
          <def_link>2084</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear>shelterGetResource(food) &gt;= 30</checkOnAppear>
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick>

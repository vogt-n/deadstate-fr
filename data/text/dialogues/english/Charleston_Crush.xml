          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>Why don't you just tell her?</text>
          <def_link>4375</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>

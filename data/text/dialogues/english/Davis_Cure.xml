    <dlgPart>
      <lines>
        <dlgLine>
          <owner_ID>0</owner_ID>
          <text>I... I never thought I'd say this, but... for now, I agree.</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

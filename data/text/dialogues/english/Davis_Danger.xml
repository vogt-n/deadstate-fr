        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>Thanks for the warning. I'll try to improve things.</text>
          <def_link>7710</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />

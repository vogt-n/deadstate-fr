            <boolean>false</boolean>
          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>Peut-être parce qu'ils croient sincèrement que les choses s'améliorent au jour le jour.</text>
          <def_link>2439</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>Être malheureux tout le temps ne mène nulle part.</text>
          <def_link>2439</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>Parce qu'ils ne se nourrissent pas de leur propre souffrance. Tu sais, ils sont normaux.</text>
          <def_link>2439</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />

    <dlgPart>
      <lines>
        <dlgLine>
          <owner_ID>0</owner_ID>
          <text>My fiancee... we both worked at a school.</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>

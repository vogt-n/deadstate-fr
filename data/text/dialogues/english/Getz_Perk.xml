            <string>removePerk(Trait_Getz_Unhinged, 1014);</string>
            <string />
          </onLoadScripts>
        </dlgLine>
        <dlgLine>
          <owner_ID>0</owner_ID>
          <text>I can't forget what happened... but for the sake of the people here, I'll try not to live in that moment anymore.</text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>I'm glad I could help you through this. We'll talk more later.</text>
          <def_link>-1</def_link>
          <auto_answer>true</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick>
            <string>dlgEndFadeFullFast();</string>
          </scriptsOnClick>
        </dlgAnsw>
        <dlgAnsw>
          <text>We all have stories like yours. Excuse me.</text>
          <def_link>-1</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick>
            <string>dlgEndFadeFullFast();</string>
          </scriptsOnClick>
        </dlgAnsw>

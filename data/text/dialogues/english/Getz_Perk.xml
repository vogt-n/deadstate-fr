    <dlgPart>
      <lines>
        <dlgLine>
          <owner_ID>0</owner_ID>
          <text>...Thank you - for providing me with the time to come to this decision. I think I'll stay here, at the school. </text>
          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
          </defAppearChecks>
          <onAppearChecks />

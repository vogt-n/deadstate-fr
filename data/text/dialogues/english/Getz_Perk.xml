          <defAppearChecks>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
            <boolean>false</boolean>
          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
      </lines>

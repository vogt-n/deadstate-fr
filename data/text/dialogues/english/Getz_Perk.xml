          </defAppearChecks>
          <onAppearChecks />
          <onLoadScripts />
        </dlgLine>
      </lines>
      <answers>
        <dlgAnsw>
          <text>You worked at a school? You? You were a teacher?!</text>
          <def_link>559</def_link>
          <auto_answer>false</auto_answer>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>A school? That explains a lot.</text>
          <def_link>559</def_link>
